    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "build-staging": "rm -rf dist && ng build -c=staging --prod",
        "test-firebase-staging": "npm run build-staging && firebase serve",
